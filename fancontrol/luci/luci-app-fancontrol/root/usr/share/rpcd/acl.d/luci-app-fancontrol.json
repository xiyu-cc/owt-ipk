{
  "luci-app-fancontrol": {
    "description": "Grant RPC access for LuCI fancontrol board editor",
    "read": {
      "ubus": {
        "luci.fancontrol": [ "scan", "loadBoardConfig", "loadBoardDefaults", "loadBoardSchema", "runtimeStatus" ]
      }
    },
    "write": {
      "ubus": {
        "luci.fancontrol": [ "applyBoardConfig", "serviceAction" ]
      }
    }
  }
}
